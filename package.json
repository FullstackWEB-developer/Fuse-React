{
    "name": "create-react-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "axios": "^0.17.1",
        "axios-mock-adapter": "^1.10.0",
        "core-js": "^2.5.3",
        "lodash": "^4.17.4",
        "material-ui": "^1.0.0-beta.33",
        "material-ui-icons": "^1.0.0-beta.17",
        "path-to-regexp": "^2.1.0",
        "perfect-scrollbar": "^1.3.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
